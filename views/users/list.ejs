<html>
    <head>
            <meta charset="utf-8">
            <title>Chat | ImageApp</title>
            <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"> 
             <link rel="stylesheet" href="/css/style.css">
    </head>
    <body>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% for(var i=0; i < data.length; i++)  { %>
            <tr>
                <td>
                    <%= data[i].username %>
                </td>
                <td>
                        <%= data[i].email %>
                </td>
            </tr>
        <% } %>
     
    </tbody>
</table>
</body>
</html>